    "@typescript-eslint/parser": "2.15.0",
    "babel-eslint": "10.0.1",
    "cross-env": "6.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "font-awesome": "^4.7.0",
    "husky": "4.0.6",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "npm-run-all": "4.1.5",
