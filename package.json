{
  "name": "node-boilerplate",
  "description": "Plan Market Node Mongo",
  "author": "Papagayo Dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node-dev --respawn --clear --transpileOnly src/server.ts",
    "dev": "npm start",
    "start:dev": "npm run build && NODE_ENV=development node build/server.js",
    "start:stage": "npm run build && NODE_ENV=staging node build/server.js",
    "start:prod": "npm run build && NODE_ENV=production node build/server.js",
    "test": "cross-env NODE_ENV=test jest",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|jsx|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "build": "tsc",
    "validate": "npm-run-all --parallel build check-format lint"
  },
  "dependencies": {
    "@azure/identity": "^1.5.2",
    "@azure/keyvault-secrets": "^4.3.0",
    "@types/bcrypt": "3.0.0",
    "@types/swagger-ui-express": "4.1.1",
    "app-root-path": "^3.0.0",
    "async-wrapper-express-ts": "3.1.1",
    "axios": "0.19.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "canvas": "^2.8.0",
    "compression": "1.7.2",
    "conversor-numero-a-letras-es-ar": "^1.0.3",
    "cors": "2.8.5",
    "cron": "1.7.2",
    "crypto-js": "^4.1.1",
    "dotenv-flow": "3.1.0",
    "express": "4.16.3",
    "helmet": "3.15.0",
    "jsonwebtoken": "8.3.0",
    "ldapjs": "^2.2.3",
    "ldapjs-client": "^0.1.1",
    "moment": "2.22.2",
    "mongodb": "3.3.3",
    "mongoose": "5.7.9",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "password-validator": "5.0.3",
    "qr-image": "^3.2.0",
    "swagger-ui-express": "4.1.3",
    "ts-node-dev": "1.0.0-pre.44",
    "typedi": "0.8.0",
    "validator": "10.8.0",
    "validatorjs": "3.17.1",
    "voca": "1.4.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
